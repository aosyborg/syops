#!/bin/bash

# Postinst script for syops

case "$1" in
    configure)
        chmod +x /etc/init.d/syops
        /etc/init.d/apache2 restart
        /etc/init.d/syops restart
    ;;

    abort-upgrade|abort-remove|abort-deconfigure)
        exit 0
    ;;

    *)
        echo "postinst called with unknown argument \`$1'" >&2
        exit 1
    ;;
esac
